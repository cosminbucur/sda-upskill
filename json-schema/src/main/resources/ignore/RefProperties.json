{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$schemaId": "ast.v1.IceHockeyEventArgumentsStored",
  "$owner": "Kambi_Dev_Wild@kambi.com",
  "$topics": [
    "dp.v2.internal.ast.EventArgumentsStored"
  ],
  "title": "Event arguments stored",
  "description": "Contains all event arguments saved or updated. This are for all sports, clients can filter for interested sport.",
  "type": "object",
  "required": [
    "eventId",
    "matchPredictions"
  ],
  "properties": {
    "eventId": {
      "type": "integer",
      "format": "int64",
      "description": "Kambi event ID"
    },
    "matchPredictions": {
      "type": "object",
      "properties": {
        "overUnder": {
          "$ref": "../schema/GenericDefinitions.json#/definitions/OverUnderLineParameters"
        },
        "oneXTwo": {
          "$ref": "../schema/GenericDefinitions.json#/definitions/OneXTwoParameters"
        }
      },
      "required": [
        "overUnder",
        "oneXTwo"
      ]
    }
  }
}