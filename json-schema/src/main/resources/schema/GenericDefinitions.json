{
  "$schema": "http://json-schema.org/schema#",
  "definitions": {
    "Status": {
      "type": "string",
      "enum": [
        "ACTIVE",
        "INACTIVE"
      ]
    },
    "Time": {
      "type": "object",
      "properties": {
        "minutes": {
          "type": "integer"
        },
        "seconds": {
          "type": "integer"
        }
      },
      "required": [
        "minutes",
        "seconds"
      ]
    }
  }
}